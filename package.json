{
  "name": "u-kirche",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build:dev": "ng build --configuration development",
    "watch:dev": "ng build --watch --configuration development",
    "build:prod": "ng build --configuration production",
    "watch:prod": "ng build --watch --configuration production",
    "format": "npx prettier src --check",
    "format:fix": "npx prettier src --write",
    "deploy:dev": "npm run build:dev && firebase deploy --only hosting",
    "deploy:prod": "npm run build:prod && firebase deploy --only hosting --project prod",
    "serve:ssr:u-kirche": "node dist/u-kirche/server/server.mjs",
    "test:e2e": "npx playwright test"
  },
  "private": true,
  "dependencies": {
    "@angular-eslint/schematics": "^19.0.2",
    "@angular/animations": "^17.2.0",
    "@angular/common": "^17.2.0",
    "@angular/compiler": "^17.2.0",
    "@angular/core": "^17.2.0",
    "@angular/forms": "^17.2.0",
    "@angular/localize": "^17.2.0",
    "@angular/platform-browser": "^17.2.0",
    "@angular/platform-browser-dynamic": "^17.2.0",
    "@angular/platform-server": "^17.2.0",
    "@angular/router": "^17.2.0",
    "@angular/ssr": "^17.3.11",
    "@ngrx/effects": "^17.0.0",
    "@ngrx/store": "^17.0.0",
    "@ngx-translate/core": "^16.0.3",
    "@ngx-translate/http-loader": "^16.0.0",
    "express": "^4.18.2",
    "ng-recaptcha": "^13.2.1",
    "prettier": "^3.4.1",
    "primeng": "^17.18.11",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.2.3",
    "@angular/cli": "^17.2.3",
    "@angular/compiler-cli": "^17.2.0",
    "@playwright/test": "^1.49.1",
    "@types/express": "^4.17.17",
    "@types/node": "^18.18.0",
    "typescript": "~5.3.2"
  }
}